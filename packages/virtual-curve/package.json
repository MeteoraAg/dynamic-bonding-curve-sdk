{
  "name": "@mcsam/virtual-curve",
  "version": "0.0.0",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "main": "dist/index.cjs",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch",
    "build": "bunx bunup index.ts --format esm,cjs --dts",
    "test:create-config": "tsx tests/createConfig.test.ts",
    "test:create-pool": "tsx tests/createPool.test.ts",
    "create-config": "tsx scripts/createConfig.ts",
    "create-pool": "tsx scripts/createPool.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.mts"
    }
  },
  "devDependencies": {
    "@meteora-ag/ts-sdk-config": "*",
    "@types/bun": "latest",
    "eslint": "^9.24.0",
    "tsx": "^4.19.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.0"
  }
}